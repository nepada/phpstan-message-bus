{
    "name": "nepada/phpstan-message-bus",
    "type": "phpstan-extension",
    "license": "BSD-3-Clause",
    "description": "PHPStan extension for nepada/message-bus.",
    "keywords": ["phpstan", "message bus", "nepada"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/phpstan-message-bus/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0 <8.2",
        "nepada/message-bus": "^2.0@dev",
        "nette/robot-loader": "^3.3.1@dev || ~3.2.4@dev || ~3.1.2@dev",
        "phpstan/phpstan": "^1.0"
    },
    "conflict": {
        "nette/finder": "<2.5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.21 || ^9.5.10",
        "phpstan/phpstan-strict-rules": "1.3.0",
        "spaze/phpstan-disallowed-calls": "2.5.0",
        "shipmonk/phpstan-rules": "1.0.1",
        "phpstan/phpstan-phpunit": "1.1.1",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "nepada/coding-standard": "7.5.2"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\PHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\PHPStan\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
